<% for page in @tabs -%>
	<% unless page.name == 'home' %>
		<% if page.name == 'properties' %>
			<li id="nav_<%= page.name %>"><%= link_to 'Find your Home', properties_path %><%= image_tag("system/buttons/nav_arrow.png", :class=>"arrow") %></li>
		<% else %>
			<li id="nav_<%= page.name %>"><%= link_to page.navlabel, view_page_path(page.name)%> <%= image_tag("system/buttons/nav_arrow.png", :class=>"arrow") %></li>
		<% end %>	
	<% end %>
<% end -%>
