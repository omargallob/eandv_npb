<div id="main">
	<h1>Editing page</h1>

	<% form_for @page, :url => {:action => "update"} do |f| %>
	  <%= render :partial => "form", :locals => {:f =>f}%>
	  <p>
	    <%= f.submit 'Update' %>
	  </p>
	<% end %>

	<%= link_to 'Show', admin_page_path(@page) %> |
	<%= link_to 'Back', admin_pages_path %>
</div>