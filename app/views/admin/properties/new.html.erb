<div id="side-nav">
	<ul>
		<%= render :partial => "shared/admin/property_sidenav" %>
	</ul>
</div>

<div id="main">
	<h1>New property for Orange County (CA)</h1>

	
	<div id="tabs" class="ui-tabs">
	    <ul >
	        <li ><a href="#fragment-1"><span>Info</span></a></li>
	        <li><a href="#fragment-2"><span>Extra</span></a></li>
	        <li><a href="#fragment-3"><span>Images</span></a></li>
	    </ul>
	 
      	 <% form_for :property, :url => {:action => "create"}  do |f| %>	
		 <%= f.error_messages %> 
		  <%= render :partial => "form", :locals => {:f =>f}%>

		<% end %>
		
	    <div id="fragment-3">
	      	<% form_for(:featured_photo, :url => {:controller=>"properties", :action => "save_featured_photo"}, :html => { :multipart => true }) do |form| %>
				<p>Featured<br />
				<%= form.file_field :uploaded_data %>
				</p>
			   <p><%= form.submit 'Update' %></p>
			<% end %>
	    </div>
	</div>
	
</div>
<% content_for :script do %>
<script type='text/javascript'>
$(document).ready(function(){
  $("#tabs").tabs();
});
	
</script>
<% end %>