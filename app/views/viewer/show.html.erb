<div id="side-nav">

	<% unless @subpages.empty? %>
	<ul>
		<% for page in @subpages %>
			<li><%= link_to page.navlabel, view_page_path(page.name)%></li>
		<% end %>
	</ul>
	<% end %>
	<%# image_tag "static/gallery.png", :style => "margin:15px 15px 5px" %>


		<% if @subpages.empty? %>
		<ul id="gallerylinks">
			<%= render :partial => "shared/gallerylinks" %>
		</ul>	
		<% end %>

</div>


<div id="main">
	<h1><%= @page.title %> </h1>
	<%= @page.body %>
</div>


<% if @page.name == "home" %>
<div id="searchbox">
	<%= render :partial => "shared/quicksearchbox" %>
</div>
<% end %>
